{
    "editor.rulers": [80, 100],
    "editor.fontSize": 17,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.wordWrap": "off",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "commands": ["vspacecode.space"],
        "before": ["<space>"]
      },
      {
        "commands": [
          "vspacecode.space",
          {
            "command": "whichkey.triggerKey",
            "args": "m"
          }
        ],
        "before": [","]
      }
    ],
    "vim.surround": true,
    "eslint.debug": true,
    "editor.formatOnSave": true,
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vim.replaceWithRegister": true,
    "vim.useSystemClipboard": true,
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.fontLigatures": true,
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.colorCustomizations": {
      "statusBar.background": "#005f87",
      "[Bluloco Dark]": {
        "statusBar.background": "#333"
      },
      "statusBar.noFolderBackground": "#005f87",
      "statusBar.debuggingBackground": "#005f87",
      "titleBar.activeForeground": "#fff",
      "titleBar.inactiveForeground": "#fff",
      "titleBar.activeBackground": "#e91e63",
      "titleBar.inactiveBackground": "#e91e63",
      "statusBar.foreground": "#ffffff",
      "statusBar.debuggingForeground": "#ffffff"
    },
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.glyphMargin": false,
    "files.trimTrailingWhitespace": true,
    "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vim.visualModeKeyBindingsNonRecursive": [
      {
        "commands": ["vspacecode.space"],
        "before": ["<space>"]
      },
      {
        "commands": [
          "vspacecode.space",
          {
            "command": "whichkey.triggerKey",
            "args": "m"
          }
        ],
        "before": [","]
      }
    ],
    "vim.easymotion": true,
    "cSpell.userWords": [
      "alfredworkflow",
      "alfy",
      "Allstar",
      "amerisave",
      "bankofkc",
      "bloginfo",
      "bluehomeloans",
      "browserslist",
      "Cashout",
      "chash",
      "cobrand",
      "composability",
      "craco",
      "credgrade",
      "DOTFILES",
      "expid",
      "foldl",
      "foldr",
      "freedommortgage",
      "frontlinefinancial",
      "fullbeaker",
      "geogate",
      "gtag",
      "guaranteedrate",
      "ifreedom",
      "linktext",
      "loandepot",
      "loanpurpose",
      "localstorage",
      "magnoliabank",
      "militaryvaloan",
      "mortgagebalance",
      "morty",
      "newamerican",
      "nextjs",
      "NMLS",
      "norg",
      "paddio",
      "paramountbank",
      "pennymac",
      "preuninstall",
      "propertyvalue",
      "purchaseprice",
      "radiusfinancial",
      "ramda",
      "ratequote",
      "redirections",
      "semibold",
      "Shortcode",
      "shortcodes",
      "tailwindcss",
      "testid",
      "themortgagereports",
      "Todoist",
      "Todos",
      "toggl",
      "Transitionable",
      "typecheck",
      "unitedmil",
      "unmount",
      "unwords",
      "upsell",
      "uuidv",
      "vercel",
      "vunited",
      "wellsfargo",
      "yabai",
      "Youtube"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "editor.lineNumbers": "off",
    "editor.accessibilitySupport": "off",
    "terminal.integrated.env.osx": {
      "FIG_NEW_SESSION": "1"
    },
    "editor.fontFamily": "FiraCode Nerd Font Mono, monospace",
    "vim.camelCaseMotion.enable": true,
    "editor.renderWhitespace": "none",
    "vim.highlightedyank.color": "rgb(135 181 255 / 50%)",
    "vim.highlightedyank.enable": true,
    "vim.sneak": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.statusBarColorControl": true,
    "[markdown]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.editor.showTabs": "single",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vim.foldfix": true,
    "editor.folding": false,
    "workbench.editor.enablePreview": false,
    "editor.minimap.enabled": false,
    "[python]": {
      "editor.formatOnType": true
    },
    "workbench.preferredLightColorTheme": "Default Light Modern",
    "workbench.colorTheme": "Main Theme / Custom Github Dark Dimmed",
    "workbench.preferredDarkColorTheme": "Sweet Candy Color Theme",
    "editor.inlineSuggest.enabled": true,
    "symbols.hidesExplorerArrows": false,
    "haskell.manageHLS": "PATH",
    "editor.unicodeHighlight.includeStrings": false,
    "window.commandCenter": false,
    "editor.unicodeHighlight.allowedCharacters": {
      "’": true,
      "️": true,
      "ℹ": true
    },
    "prisma.showPrismaDataPlatformNotification": false,
    "workbench.activityBar.location": "hidden",
    "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
    "window.titleBarStyle": "native",
    "workbench.layoutControl.enabled": false,
    "workbench.iconTheme": "vscode-great-icons",
    "settings.cycle": [
      {
        "id": "lineNumbers",
        "values": [
          {
            "editor.lineNumbers": "on"
          },
          {
            "editor.lineNumbers": "off"
          }
        ]
      }
    ],
    "vspacecode.bindingOverrides": [
      {
        "keys": "t.n",
        "position": 2,
        "name": "Toggle line numbers",
        "type": "command",
        "command": "settings.cycle.lineNumbers"
      }
    ],
    "window.systemColorTheme": "auto",
    "window.autoDetectColorScheme": true,
    "security.promptForLocalFileProtocolHandling": false,
    "window.customTitleBarVisibility": "never"
  }
